    "betterScripts": {
        "dev": {
            "command": "npm-run-all hosts:aliases build:configs build:server && npm-run-all --parallel --print-name watch:ssr start:dev",
            "env": {
                "NODE_ENV": "development"
            }
        },
        "build:production": {
            "command": "npm-run-all build:configs build:server build:production:vendors build:ssr build:production:client",
            "env": {
                "NODE_ENV": "production"
            }
        },
        "build:production:vendors": {
            "command": "webpack --mode=production --hide-modules --config webpack/config/vendors.config.ts",
            "env": {
                "NODE_ENV": "production"
            }
        },
        "build:production:client": {
            "command": "webpack --mode=production --hide-modules --config webpack/config/client.config.ts",
            "env": {
                "NODE_ENV": "production"
            }
        },
        "watch:ssr": {
            "command": "webpack --watch --hide-modules --info-verbosity none --config webpack/config/ssr.config.ts",
            "env": {
                "NODE_ENV": "development"
            }
        },
        "hosts:aliases": {
            "command": "WHO_AM_I=$(whoami) ts-node scripts/makeAliases.ts"
        }
    },
    "browserslist": [
        "> 1%",
        "ie > 8"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "npm run precommit"
        }
    }
